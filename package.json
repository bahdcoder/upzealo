{
  "name": "upzealo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/expect": "^1.1.4",
    "@japa/preset-adonis": "^1.1.0",
    "@japa/runner": "^2.0.9",
    "@types/sinon": "^10.0.13",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "typescript": "~4.6",
    "youch": "^3.2.0",
    "youch-terminal": "^2.1.4"
  },
  "dependencies": {
    "@adonisjs/ally": "^4.1.1",
    "@adonisjs/auth": "^8.2.1",
    "@adonisjs/core": "^5.8.5",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/lucid-slugify": "2.1.4",
    "@adonisjs/redis": "^7.3.0",
    "@adonisjs/repl": "^3.1.11",
    "@bahdcoder/ally-twitter-v2": "^1.0.3",
    "@metaplex-foundation/js": "^0.14.1",
    "@project-serum/anchor": "^0.25.0",
    "@rocketseat/adonis-bull": "^1.0.4",
    "@solana/spl-token": "^0.2.0",
    "@solana/web3.js": "^1.50.0",
    "@types/bs58": "^4.0.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/puppeteer": "^5.4.6",
    "adonis5-jwt": "^1.1.6",
    "bs58": "^5.0.0",
    "classnames": "^2.3.1",
    "cloudinary": "^1.30.1",
    "convert-string-to-number": "^1.0.3",
    "edge.js": "^5.3.7",
    "getstream": "^8.0.1",
    "html-pdf-node": "^1.0.8",
    "humanize-duration": "^3.27.2",
    "ksuid": "^3.0.0",
    "luxon": "^3.0.1",
    "mysql": "^2.18.1",
    "node-fetch": "2",
    "proxy-addr": "^2.0.7",
    "puppeteer": "^16.1.0",
    "react-images-uploading": "^3.1.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  }
}
